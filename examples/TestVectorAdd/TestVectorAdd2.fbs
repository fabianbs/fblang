global public int[] vecAdd(int[] arr1, arr2){
    uint len = arr1.length > arr2.length ? arr2.length : arr1.length
    int[] arr3 = new [len]
    for simd(uint i: 0 to len){
        arr3[i] = arr1[i] + arr2[i]
    }
    return arr3
}